{
   "applications": {
      "gecko": {
         "id": "{a4c4eda4-fb84-4a84-b4a1-f7c1cbf2a1ad}",
         "strict_min_version": "84.0"
      }
   },
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png"
   },
   "content_scripts": [ {
      "css": [ "refined-github.css" ],
      "exclude_matches": [ "https://*/login/*" ],
      "js": [ "refined-github.js" ],
      "matches": [ "https://github.com/*", "https://gist.github.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Simplifies the GitHub interface and adds useful features",
   "homepage_url": "https://github.com/refined-github/refined-github",
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "88",
   "name": "Refined GitHub",
   "optional_permissions": [ "*://*/*" ],
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "storage", "contextMenus", "activeTab", "alarms", "https://github.com/*", "https://api.github.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "22.5.22",
   "web_accessible_resources": [ "resolve-conflicts.js" ]
}
